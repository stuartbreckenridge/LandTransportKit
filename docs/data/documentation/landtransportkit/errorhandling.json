deListing","code":["import SwiftUI","import LandTransportKit","","struct DataView: View {","    @State private var data: [BusStop] = []","    @State private var error: LandTransportAPIError?","    @State private var isLoading = false","    ","    var body: some View {","        Group {","            if isLoading {","                ProgressView(\"Loading...\")","            } else if let error = error {","                errorView(for: error)","            } else {","                dataList","            }","        }","        .task {","            await loadData()","        }","    }","    ","    private var dataList: some View {","        List(data, id: \\.id) { stop in","            Text(stop.Description)","        }","    }","    ","    @ViewBuilder","    private func errorView(for error: LandTransportAPIError) -> some View {","        ContentUnavailableView {","            Label(errorTitle(for: error), systemImage: errorIcon(for: error))","        } description: {","            Text(error.localizedDescription)","        } actions: {","            Button(\"Try Again\") {","                Task { await loadData() }","            }","        }","    }","    ","    private func errorTitle(for error: LandTransportAPIError) -> String {","        switch error {","        case .noAPIKey:","            return \"Not Configured\"","        case .rateLimited:","            return \"Too Many Requests\"","        case .networkError:","            return \"Connection Error\"","        default:","            return \"Error\"","        }","    }","    ","    private func errorIcon(for error: LandTransportAPIError) -> String {","        switch error {","        case .noAPIKey:","            return \"key.slash\"","        case .rateLimited:","            return \"clock.badge.exclamationmark\"","        case .networkError:","            return \"wifi.slash\"","        default:","            return \"exclamationmark.triangle\"","        }","    }","    ","    private func loadData() async {","        isLoading = true","        error = nil","        defer { isLoading = false }","        ","        do {","            data = try await LandTransportAPI.shared.downloadBusStops()","        } catch let apiError as LandTransportAPIError {","            error = apiError","        } catch {","            \/\/ Convert unexpected errors","            self.error = .networkError(underlying: URLError(.unknown))","        }","    }","}"],"syntax":"swift"},{"type":"heading","text":"Error Logging","level":2,"anchor":"Error-Logging"},{"type":"paragraph","inlineContent":[{"type":"text","text":"For production apps, consider logging errors for debugging:"}]},{"type":"codeListing","syntax":"swift","code":["import os.log","","let logger = Logger(subsystem: \"com.myapp\", category: \"API\")","","func loadData() async {","    do {","        let data = try await LandTransportAPI.shared.downloadBusStops()","        logger.info(\"Loaded \\(data.count) bus stops\")","    } catch let error as LandTransportAPIError {","        logger.error(\"API error: \\(error.localizedDescription)\")","        \/\/ Handle error...","    } catch {","        logger.error(\"Unexpected error: \\(error.localizedDescription)\")","        \/\/ Handle error...","    }","}"]},{"type":"heading","level":2,"text":"Testing Error Handling","anchor":"Testing-Error-Handling"},{"type":"paragraph","inlineContent":[{"text":"You can create a custom ","type":"text"},{"type":"codeVoice","code":"LandTransportAPI"},{"text":" instance with a mock ","type":"text"},{"type":"codeVoice","code":"URLSession"},{"text":" to test error handling:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["import XCTest","@testable import LandTransportKit","","class ErrorHandlingTests: XCTestCase {","    ","    func testNoAPIKeyError() async {","        \/\/ Create an instance without configuring API key","        let api = LandTransportAPI(urlSession: URLSession.shared)","        ","        do {","            _ = t