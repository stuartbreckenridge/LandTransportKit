{"identifier":{"url":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/FacilitiesAPIs","interfaceLanguage":"swift"},"seeAlsoSections":[{"title":"Facilities & Other","generated":true,"identifiers":["doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/CarPark","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BikePark","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/EstimatedTravelTime","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/FaultyTrafficLight","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/RoadEvent","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LiftMaintenance"],"anchor":"Facilities--Other"}],"topicSections":[{"title":"Models","identifiers":["doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/CarPark","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BikePark","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/EstimatedTravelTime","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/FaultyTrafficLight","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/RoadEvent","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LiftMaintenance"],"anchor":"Models"}],"hierarchy":{"paths":[["doc:\/\/LandTransportKit\/documentation\/LandTransportKit"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"kind":"article","abstract":[{"type":"text","text":"Access car park availability, bike parking, travel times, and more."}],"primaryContentSections":[{"content":[{"type":"heading","anchor":"Overview","level":2,"text":"Overview"},{"type":"paragraph","inlineContent":[{"type":"text","text":"LandTransportKit provides access to various facility and utility APIs:"}]},{"type":"table","header":"row","rows":[[[{"inlineContent":[{"type":"text","text":"API"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Method"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Description"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"Car Park Availability"}],"type":"paragraph"}],[{"inlineContent":[{"isActive":true,"type":"reference","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadCarParkAvailability()"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Real-time parking lot counts"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"Bike Parking"}],"type":"paragraph"}],[{"inlineContent":[{"isActive":true,"type":"reference","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/getBikeParks(near:long:radius:)"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Bicycle parking locations"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"Estimated Travel Time"}],"type":"paragraph"}],[{"inlineContent":[{"isActive":true,"type":"reference","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadEstimatedTravelTimes()"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Highway travel times"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"Faulty Traffic Lights"}],"type":"paragraph"}],[{"inlineContent":[{"isActive":true,"type":"reference","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadFaultyTrafficLights()"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Traffic light issues"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"Road Events"}],"type":"paragraph"}],[{"inlineContent":[{"isActive":true,"type":"reference","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadRoadOpenings()"},{"type":"text","text":" \/ "},{"isActive":true,"type":"reference","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadRoadWorks()"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Road works and openings"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"Facilities Maintenance"}],"type":"paragraph"}],[{"inlineContent":[{"isActive":true,"type":"reference","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadFacilitiesMaintenance()"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Lift maintenance info"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"Passenger Volume"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Various methods"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Historical ridership data"}],"type":"paragraph"}]]]},{"type":"heading","level":2,"anchor":"Car-Park-Availability","text":"Car Park Availability"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The car park availability API provides real-time information about parking lot availability across Singapore."}]},{"type":"heading","level":3,"anchor":"Basic-Usage","text":"Basic Usage"},{"type":"codeListing","code":["import LandTransportKit","","let carParks = try await LandTransportAPI.shared.downloadCarParkAvailability()","print(\"Total car parks: \\(carParks.count)\")"],"syntax":"swift"},{"type":"aside","name":"Note","content":[{"inlineContent":[{"type":"text","text":"This is a paginated endpoint. The API automatically fetches all pages."}],"type":"paragraph"}],"style":"note"},{"type":"heading","level":3,"anchor":"Car-Park-Details","text":"Car Park Details"},{"type":"codeListing","code":["let carPark = carParks.first!","","print(\"ID: \\(carPark.CarParkID)\")","print(\"Area: \\(carPark.Area)\")","print(\"Development: \\(carPark.Development)\")","print(\"Available Lots: \\(carPark.AvailableLots)\")","print(\"Lot Type: \\(carPark.LotType)\")   \/\/ \"C\" = Cars, \"Y\" = Motorcycles, \"H\" = Heavy Vehicles","print(\"Agency: \\(carPark.Agency)\")      \/\/ \"HDB\", \"LTA\", \"URA\"","","\/\/ Get location","if let coordinate = carPark.coordinate {","    print(\"Location: \\(coordinate.latitude), \\(coordinate.longitude)\")","}"],"syntax":"swift"},{"type":"heading","level":3,"anchor":"Finding-Nearby-Car-Parks","text":"Finding Nearby Car Parks"},{"type":"codeListing","code":["import CoreLocation","","func findNearbyCarParks(near location: CLLocation, radius: CLLocationDistance = 500) async throws -> [CarPark] {","    let allCarParks = try await LandTransportAPI.shared.downloadCarParkAvailability()","    ","    return allCarParks","        .filter { carPark in","            guard let coordinate = carPark.coordinate else { return false }","            let carParkLocation = CLLocation(latitude: coordinate.latitude, longitude: coordinate.longitude)","            return carParkLocation.distance(from: location) <= radius","        }","        .sorted { cp1, cp2 in","            guard let coord1 = cp1.coordinate, let coord2 = cp2.coordinate else { return false }","            let loc1 = CLLocation(latitude: coord1.latitude, longitude: coord1.longitude)","            let loc2 = CLLocation(latitude: coord2.latitude, longitude: coord2.longitude)","            return loc1.distance(from: location) < loc2.distance(from: location)","        }","}"],"syntax":"swift"},{"type":"heading","level":3,"anchor":"Filtering-by-Lot-Type","text":"Filtering by Lot Type"},{"type":"codeListing","code":["\/\/ Get car parks with car lots","let carLots = carParks.filter { $0.LotType == \"C\" }","","\/\/ Get car parks with motorcycle lots","let motorcycleLots = carParks.filter { $0.LotType == \"Y\" }","","\/\/ Get car parks with available spaces","let availableParks = carParks.filter { $0.AvailableLots > 0 }"],"syntax":"swift"},{"type":"heading","level":3,"anchor":"Car-Park-Availability-View","text":"Car Park Availability View"},{"type":"codeListing","code":["import SwiftUI","import LandTransportKit","","struct CarParkListView: View {","    @State private var carParks: [CarPark] = []","    @State private var searchText = \"\"","    @State private var isLoading = false","    ","    var filteredCarParks: [CarPark] {","        if searchText.isEmpty {","            return carParks","        }","        return carParks.filter {","            $0.Development.localizedCaseInsensitiveContains(searchText) ||","            $0.Area.localizedCaseInsensitiveContains(searchText)","        }","    }","    ","    var body: some View {","        NavigationStack {","            List(filteredCarParks, id: \\.id) { carPark in","                HStack {","                    VStack(alignment: .leading) {","                        Text(carPark.Development)","                            .font(.headline)","                        Text(carPark.Area)","                            .font(.subheadline)","                            .foregroundStyle(.secondary)","                    }","                    Spacer()","                    VStack(alignment: .trailing) {","                        Text(\"\\(carPark.AvailableLots)\")","                            .font(.title2.bold())","                            .foregroundStyle(availabilityColor(carPark.AvailableLots))","                        Text(\"lots\")","                            .font(.caption)","                            .foregroundStyle(.secondary)","                    }","                }","            }","            .searchable(text: $searchText, prompt: \"Search car parks\")","            .navigationTitle(\"Car Parks\")","            .refreshable {","                await loadCarParks()","            }","            .task {","                await loadCarParks()","            }","        }","    }","    ","    private func availabilityColor(_ lots: Int) -> Color {","        switch lots {","        case 0: return .red","        case 1...10: return .orange","        case 11...50: return .yellow","        default: return .green","        }","    }","    ","    private func loadCarParks() async {","        isLoading = true","        defer { isLoading = false }","        ","        do {","            carParks = try await LandTransportAPI.shared.downloadCarParkAvailability()","        } catch {","            print(\"Error: \\(error)\")","        }","    }","}"],"syntax":"swift"},{"type":"heading","level":2,"anchor":"Bike-Parking","text":"Bike Parking"},{"inlineContent":[{"type":"text","text":"The bike parking API returns bicycle parking locations near a specified coordinate."}],"type":"paragraph"},{"text":"Basic Usage","anchor":"Basic-Usage","type":"heading","level":3},{"type":"codeListing","code":["\/\/ Find bike parking within 500m of a location","let bikeParks = try await LandTransportAPI.shared.getBikeParks(","    near: 1.3521,      \/\/ latitude","    long: 103.8198,    \/\/ longitude","    radius: 0.5        \/\/ radius in km (default)",")"],"syntax":"swift"},{"level":3,"anchor":"Bike-Park-Details","type":"heading","text":"Bike Park Details"},{"type":"codeListing","code":["let bikePark = bikeParks.first!","","print(\"Description: \\(bikePark.Description)\")","print(\"Rack Type: \\(bikePark.RackType)\")","print(\"Rack Count: \\(bikePark.RackCount)\")","print(\"Shelter: \\(bikePark.ShelterIndicator)\")","","\/\/ Location","let location = bikePark.location","print(\"Location: \\(location.coordinate.latitude), \\(location.coordinate.longitude)\")"],"syntax":"swift"},{"type":"heading","level":3,"anchor":"Finding-Bike-Parking-Near-MRT-Stations","text":"Finding Bike Parking Near MRT Stations"},{"code":["import CoreLocation","","\/\/ Example: Find bike parking near Orchard MRT","let orchardMRT = CLLocationCoordinate2D(latitude: 1.3039, longitude: 103.8318)","","let nearbyBikeParks = try await LandTransportAPI.shared.getBikeParks(","    near: orchardMRT.latitude,","    long: orchardMRT.longitude,","    radius: 0.3  \/\/ 300 meters",")","","for park in nearbyBikeParks {","    print(\"\\(park.Description): \\(park.RackCount) racks\")","}"],"type":"codeListing","syntax":"swift"},{"type":"heading","level":2,"anchor":"Estimated-Travel-Time","text":"Estimated Travel Time"},{"inlineContent":[{"type":"text","text":"The estimated travel time API provides current travel times for major expressway routes."}],"type":"paragraph"},{"type":"heading","level":3,"anchor":"Basic-Usage","text":"Basic Usage"},{"type":"codeListing","code":["let travelTimes = try await LandTransportAPI.shared.downloadEstimatedTravelTimes()","","for route in travelTimes {","    print(\"\\(route.Name): \\(route.EstTime) minutes\")","}"],"syntax":"swift"},{"type":"heading","level":3,"anchor":"Travel-Time-Details","text":"Travel Time Details"},{"type":"codeListing","code":["let route = travelTimes.first!","","print(\"Route: \\(route.Name)\")","print(\"Direction: \\(route.Direction)\")","print(\"Far End Point: \\(route.FarEndPoint)\")","print(\"Estimated Time: \\(route.EstTime) minutes\")"],"syntax":"swift"},{"type":"heading","level":3,"anchor":"Travel-Time-Display","text":"Travel Time Display"},{"type":"codeListing","code":["import SwiftUI","import LandTransportKit","","struct TravelTimesView: View {","    @State private var travelTimes: [EstimatedTravelTime] = []","    ","    var body: some View {","        List(travelTimes, id: \\.id) { route in","            HStack {","                VStack(alignment: .leading) {","                    Text(route.Name)","                        .font(.headline)","                    Text(\"To \\(route.FarEndPoint)\")","                        .font(.subheadline)","                        .foregroundStyle(.secondary)","                }","                Spacer()","                Text(\"\\(route.EstTime) min\")","                    .font(.title3.monospacedDigit())","                    .foregroundStyle(.blue)","            }","        }","        .navigationTitle(\"Travel Times\")","        .task {","            do {","                travelTimes = try await LandTransportAPI.shared.downloadEstimatedTravelTimes()","            } catch {","                print(\"Error: \\(error)\")","            }","        }","    }","}"],"syntax":"swift"},{"level":2,"anchor":"Faulty-Traffic-Lights","type":"heading","text":"Faulty Traffic Lights"},{"inlineContent":[{"type":"text","text":"The faulty traffic lights API reports current traffic light malfunctions and scheduled maintenance."}],"type":"paragraph"},{"level":3,"anchor":"Basic-Usage","type":"heading","text":"Basic Usage"},{"code":["let faults = try await LandTransportAPI.shared.downloadFaultyTrafficLights()","","for fault in faults {","    print(\"\\(fault.AlarmID): \\(fault.TechnicalAlarmType) at \\(fault.NodeID)\")","}"],"type":"codeListing","syntax":"swift"},{"level":3,"anchor":"Fault-Details","type":"heading","text":"Fault Details"},{"code":["let fault = faults.first!","","print(\"Alarm ID: \\(fault.AlarmID)\")","print(\"Node ID: \\(fault.NodeID)\")","print(\"Type: \\(fault.TechnicalAlarmType)\")","print(\"Start Date: \\(fault.StartDate)\")","print(\"End Date: \\(fault.EndDate)\")","print(\"Message: \\(fault.Message)\")","","\/\/ Check if this is scheduled maintenance","if fault.isScheduledMaintenance {","    print(\"This is scheduled maintenance\")","}","","\/\/ Get dates as Date objects","if let startDate = fault.iso8601StartDate {","    print(\"Started: \\(startDate)\")","}"],"type":"codeListing","syntax":"swift"},{"level":2,"anchor":"Road-Events","type":"heading","text":"Road Events"},{"inlineContent":[{"type":"text","text":"The road events APIs provide information about road openings and road works."}],"type":"paragraph"},{"level":3,"anchor":"Road-Openings","type":"heading","text":"Road Openings"},{"code":["let openings = try await LandTransportAPI.shared.downloadRoadOpenings()","","for opening in openings {","    print(\"Event: \\(opening.EventID)\")","    print(\"Start: \\(opening.StartDate)\")","    print(\"End: \\(opening.EndDate)\")","}"],"type":"codeListing","syntax":"swift"},{"level":3,"anchor":"Road-Works","type":"heading","text":"Road Works"},{"code":["let works = try await LandTransportAPI.shared.downloadRoadWorks()","","for work in works {","    print(\"Event: \\(work.EventID)\")","    print(\"Road: \\(work.RoadName)\")","    print(\"Other: \\(work.Other)\")","}"],"type":"codeListing","syntax":"swift"},{"level":2,"anchor":"Facilities-Maintenance","type":"heading","text":"Facilities Maintenance"},{"inlineContent":[{"type":"text","text":"The facilities maintenance API provides information about lift maintenance at transport facilities."}],"type":"paragraph"},{"level":3,"anchor":"Basic-Usage","type":"heading","text":"Basic Usage"},{"code":["let maintenance = try await LandTransportAPI.shared.downloadFacilitiesMaintenance()","","for item in maintenance {","    print(\"Station: \\(item.Station)\")","    print(\"Building: \\(item.Building)\")","}"],"type":"codeListing","syntax":"swift"},{"level":2,"anchor":"Passenger-Volume-Data","type":"heading","text":"Passenger Volume Data"},{"inlineContent":[{"type":"text","text":"The passenger volume APIs provide historical ridership data. These return ZIP files containing CSV data."}],"type":"paragraph"},{"name":"Warning","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"These endpoints are rate-limited. Excessive requests will result in a "},{"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPIError\/rateLimited","type":"reference","isActive":true},{"type":"text","text":" error."}]}],"style":"warning","type":"aside"},{"level":3,"anchor":"Available-Endpoints","type":"heading","text":"Available Endpoints"},{"code":["\/\/ Bus passenger volume by stop","let (busStopData, filename1) = try await LandTransportAPI.shared.downloadPassengerVolumeByBusStop()","","\/\/ Bus passenger volume by origin-destination","let (odBusData, filename2) = try await LandTransportAPI.shared.downloadPassengerVolumeByOriginDestinationBusStop()","","\/\/ Train passenger volume by station","let (trainStationData, filename3) = try await LandTransportAPI.shared.downloadPassengerVolumeByTrainStation()","","\/\/ Train passenger volume by origin-destination","let (odTrainData, filename4) = try await LandTransportAPI.shared.downloadPassengerVolumeByOriginDestinationTrainStation()"],"type":"codeListing","syntax":"swift"},{"level":3,"anchor":"Handling-the-ZIP-Data","type":"heading","text":"Handling the ZIP Data"},{"inlineContent":[{"type":"text","text":"The returned data is in ZIP format containing a CSV file:"}],"type":"paragraph"},{"code":["import Foundation","","do {","    let (zipData, filename) = try await LandTransportAPI.shared.downloadPassengerVolumeByBusStop()","    ","    \/\/ Save to documents directory","    let documentsURL = FileManager.default.urls(for: .documentDirectory, in: .userDomainMask)[0]","    let fileURL = documentsURL.appendingPathComponent(filename)","    try zipData.write(to: fileURL)","    ","    print(\"Saved to: \\(fileURL.path)\")","    ","    \/\/ You can then unzip and parse the CSV as needed","    ","} catch LandTransportAPIError.rateLimited {","    print(\"Rate limited - please wait before trying again\")","} catch {","    print(\"Error: \\(error)\")","}"],"type":"codeListing","syntax":"swift"},{"level":2,"anchor":"Complete-Example","type":"heading","text":"Complete Example"},{"inlineContent":[{"type":"text","text":"Here’s a facilities dashboard combining multiple APIs:"}],"type":"paragraph"},{"code":["import SwiftUI","import LandTransportKit","","struct FacilitiesDashboardView: View {","    @State private var carParks: [CarPark] = []","    @State private var travelTimes: [EstimatedTravelTime] = []","    @State private var faultyLights: [FaultyTrafficLight] = []","    @State private var isLoading = false","    ","    var body: some View {","        NavigationStack {","            List {","                \/\/ Car Park Summary","                Section(\"Car Parks\") {","                    let available = carParks.filter { $0.AvailableLots > 0 }.count","                    let total = carParks.count","                    ","                    HStack {","                        Label(\"Available\", systemImage: \"car.fill\")","                        Spacer()","                        Text(\"\\(available) \/ \\(total)\")","                            .foregroundStyle(.secondary)","                    }","                }","                ","                \/\/ Travel Times","                Section(\"Expressway Travel Times\") {","                    ForEach(travelTimes.prefix(5), id: \\.id) { route in","                        HStack {","                            Text(route.Name)","                            Spacer()","                            Text(\"\\(route.EstTime) min\")","                                .foregroundStyle(.blue)","                        }","                    }","                }","                ","                \/\/ Faulty Traffic Lights","                Section(\"Traffic Light Issues (\\(faultyLights.count))\") {","                    if faultyLights.isEmpty {","                        Text(\"No reported issues\")","                            .foregroundStyle(.secondary)","                    } else {","                        ForEach(faultyLights.prefix(3), id: \\.AlarmID) { fault in","                            VStack(alignment: .leading) {","                                Text(fault.TechnicalAlarmType)","                                    .font(.headline)","                                Text(fault.Message)","                                    .font(.caption)","                                    .foregroundStyle(.secondary)","                            }","                        }","                    }","                }","            }","            .navigationTitle(\"Facilities\")","            .refreshable {","                await loadAllData()","            }","            .task {","                await loadAllData()","            }","        }","    }","    ","    private func loadAllData() async {","        isLoading = true","        defer { isLoading = false }","        ","        async let carParksTask = LandTransportAPI.shared.downloadCarParkAvailability()","        async let travelTimesTask = LandTransportAPI.shared.downloadEstimatedTravelTimes()","        async let faultyLightsTask = LandTransportAPI.shared.downloadFaultyTrafficLights()","        ","        do {","            let (parks, times, lights) = try await (carParksTask, travelTimesTask, faultyLightsTask)","            carParks = parks","            travelTimes = times","            faultyLights = lights","        } catch {","            print(\"Error: \\(error)\")","        }","    }","}"],"type":"codeListing","syntax":"swift"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/landtransportkit\/facilitiesapis"]}],"metadata":{"title":"Facilities & Other APIs","modules":[{"name":"LandTransportKit"}],"color":{"standardColorIdentifier":"green"},"role":"collectionGroup","roleHeading":"API Collection"},"sections":[],"references":{"doc://LandTransportKit/documentation/LandTransportKit":{"images":[{"identifier":"LTK-Red.png","type":"icon"}],"title":"LandTransportKit","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit","abstract":[{"type":"text","text":"A Swift package for accessing real-time and static data from Singapore’s Land Transport Authority (LTA) DataMall API."}],"type":"topic","url":"\/documentation\/landtransportkit","kind":"symbol","role":"collection"},"doc://LandTransportKit/documentation/LandTransportKit/LandTransportAPI/downloadFacilitiesMaintenance()":{"title":"downloadFacilitiesMaintenance()","kind":"symbol","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadFacilitiesMaintenance()","url":"\/documentation\/landtransportkit\/landtransportapi\/downloadfacilitiesmaintenance()","role":"symbol","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"downloadFacilitiesMaintenance"},{"kind":"text","text":"() "},{"kind":"keyword","text":"async"},{"kind":"text","text":" "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> ["},{"preciseIdentifier":"s:16LandTransportKit15LiftMaintenanceV","kind":"typeIdentifier","text":"LiftMaintenance"},{"kind":"text","text":"]"}],"abstract":[{"type":"text","text":"Downloads the latest facilities maintenance data from the Land Transport API."}],"type":"topic"},"LTK-Red.png":{"variants":[{"url":"\/images\/LandTransportKit\/LTK-Red.png","traits":["1x","light"]}],"type":"image","alt":null,"identifier":"LTK-Red.png"},"doc://LandTransportKit/documentation/LandTransportKit/LandTransportAPI/getBikeParks(near:long:radius:)":{"title":"getBikeParks(near:long:radius:)","kind":"symbol","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/getBikeParks(near:long:radius:)","url":"\/documentation\/landtransportkit\/landtransportapi\/getbikeparks(near:long:radius:)","role":"symbol","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"getBikeParks"},{"kind":"text","text":"("},{"kind":"externalParam","text":"near"},{"kind":"text","text":": "},{"preciseIdentifier":"s:Sd","kind":"typeIdentifier","text":"Double"},{"kind":"text","text":", "},{"kind":"externalParam","text":"long"},{"kind":"text","text":": "},{"preciseIdentifier":"s:Sd","kind":"typeIdentifier","text":"Double"},{"kind":"text","text":", "},{"kind":"externalParam","text":"radius"},{"kind":"text","text":": "},{"preciseIdentifier":"s:Sd","kind":"typeIdentifier","text":"Double"},{"kind":"text","text":") "},{"kind":"keyword","text":"async"},{"kind":"text","text":" "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> ["},{"preciseIdentifier":"s:16LandTransportKit8BikeParkV","kind":"typeIdentifier","text":"BikePark"},{"kind":"text","text":"]"}],"abstract":[{"type":"text","text":"Retrieves a list of bike park locations near the specified coordinates within a given radius."}],"ty