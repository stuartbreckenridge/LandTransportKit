{"abstract":[{"text":"Access car park availability, bike parking, travel times, and more.","type":"text"}],"variants":[{"paths":["\/documentation\/landtransportkit\/facilitiesapis"],"traits":[{"interfaceLanguage":"swift"}]}],"topicSections":[{"title":"Models","identifiers":["doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/CarPark","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BikePark","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/EstimatedTravelTime","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/FaultyTrafficLight","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/RoadEvent","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LiftMaintenance"],"anchor":"Models"}],"metadata":{"color":{"standardColorIdentifier":"green"},"title":"Facilities & Other APIs","roleHeading":"API Collection","modules":[{"name":"LandTransportKit"}],"role":"collectionGroup"},"sections":[],"schemaVersion":{"minor":3,"patch":0,"major":0},"kind":"article","identifier":{"url":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/FacilitiesAPIs","interfaceLanguage":"swift"},"seeAlsoSections":[{"generated":true,"title":"Facilities & Other","identifiers":["doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/CarPark","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BikePark","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/EstimatedTravelTime","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/FaultyTrafficLight","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/RoadEvent","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LiftMaintenance"],"anchor":"Facilities--Other"}],"primaryContentSections":[{"kind":"content","content":[{"anchor":"Overview","level":2,"text":"Overview","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"LandTransportKit provides access to various facility and utility APIs:"}]},{"rows":[[[{"type":"paragraph","inlineContent":[{"text":"API","type":"text"}]}],[{"inlineContent":[{"text":"Method","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Description","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"text":"Car Park Availability","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"isActive":true,"type":"reference","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadCarParkAvailability()"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Real-time parking lot counts","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"text":"Bike Parking","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/getBikeParks(near:long:radius:)"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"text":"Bicycle parking locations","type":"text"}]}]],[[{"type":"paragraph","inlineContent":[{"text":"Estimated Travel Time","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"isActive":true,"type":"reference","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadEstimatedTravelTimes()"}]}],[{"type":"paragraph","inlineContent":[{"text":"Highway travel times","type":"text"}]}]],[[{"inlineContent":[{"text":"Faulty Traffic Lights","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadFaultyTrafficLights()","isActive":true,"type":"reference"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Traffic light issues","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"text":"Road Events","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadRoadOpenings()","isActive":true,"type":"reference"},{"text":" \/ ","type":"text"},{"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadRoadWorks()","isActive":true,"type":"reference"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Road works and openings","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"text":"Facilities Maintenance","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadFacilitiesMaintenance()","isActive":true,"type":"reference"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Lift maintenance info","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"text":"Passenger Volume","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Various methods","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Historical ridership data","type":"text"}],"type":"paragraph"}]]],"type":"table","header":"row"},{"anchor":"Car-Park-Availability","type":"heading","level":2,"text":"Car Park Availability"},{"inlineContent":[{"text":"The car park availability API provides real-time information about parking lot availability across Singapore.","type":"text"}],"type":"paragraph"},{"anchor":"Basic-Usage","type":"heading","level":3,"text":"Basic Usage"},{"code":["import LandTransportKit","","let carParks = try await LandTransportAPI.shared.downloadCarParkAvailability()","print(\"Total car parks: \\(carParks.count)\")"],"type":"codeListing","syntax":"swift"},{"style":"note","name":"Note","content":[{"inlineContent":[{"text":"This is a paginated endpoint. The API automatically fetches all pages.","type":"text"}],"type":"paragraph"}],"type":"aside"},{"anchor":"Car-Park-Details","type":"heading","level":3,"text":"Car Park Details"},{"code":["let carPark = carParks.first!","","print(\"ID: \\(carPark.CarParkID)\")","print(\"Area: \\(carPark.Area)\")","print(\"Development: \\(carPark.Development)\")","print(\"Available Lots: \\(carPark.AvailableLots)\")","print(\"Lot Type: \\(carPark.LotType)\")   \/\/ \"C\" = Cars, \"Y\" = Motorcycles, \"H\" = Heavy Vehicles","print(\"Agency: \\(carPark.Agency)\")      \/\/ \"HDB\", \"LTA\", \"URA\"","","\/\/ Get location","if let coordinate = carPark.coordinate {","    print(\"Location: \\(coordinate.latitude), \\(coordinate.longitude)\")","}"],"type":"codeListing","syntax":"swift"},{"anchor":"Finding-Nearby-Car-Parks","level":3,"text":"Finding Nearby Car Parks","type":"heading"},{"type":"codeListing","code":["import CoreLocation","","func findNearbyCarParks(near location: CLLocation, radius: CLLocationDistance = 500) async throws -> [CarPark] {","    let allCarParks = try await LandTransportAPI.shared.downloadCarParkAvailability()","    ","    return allCarParks","        .filter { carPark in","            guard let coordinate = carPark.coordinate else { return false }","            let carParkLocation = CLLocation(latitude: coordinate.latitude, longitude: coordinate.longitude)","            return carParkLocation.distance(from: location) <= radius","        }","        .sorted { cp1, cp2 in","            guard let coord1 = cp1.coordinate, let coord2 = cp2.coordinate else { return false }","            let loc1 = CLLocation(latitude: coord1.latitude, longitude: coord1.longitude)","            let loc2 = CLLocation(latitude: coord2.latitude, longitude: coord2.longitude)","            return loc1.distance(from: location) < loc2.distance(from: location)","        }","}"],"syntax":"swift"},{"anchor":"Filtering-by-Lot-Type","text":"Filtering by Lot Type","type":"heading","level":3},{"code":["\/\/ Get car parks with car lots","let carLots = carParks.filter { $0.LotType == \"C\" }","","\/\/ Get car parks with motorcycle lots","let motorcycleLots = carParks.filter { $0.LotType == \"Y\" }","","\/\/ Get car parks with available spaces","let availableParks = carParks.filter { $0.AvailableLots > 0 }"],"syntax":"swift","type":"codeListing"},{"anchor":"Car-Park-Availability-View","level":3,"text":"Car Park Availability View","type":"heading"},{"type":"codeListing","code":["import SwiftUI","import LandTransportKit","","struct CarParkListView: View {","    @State private var carParks: [CarPark] = []","    @State private var searchText = \"\"","    @State private var isLoading = false","    ","    var filteredCarParks: [CarPark] {","        if searchText.isEmpty {","            return carParks","        }","        return carParks.filter {","            $0.Development.localizedCaseInsensitiveContains(searchText) ||","            $0.Area.localizedCaseInsensitiveContains(searchText)","        }","    }","    ","    var body: some View {","        NavigationStack {","            List(filteredCarParks, id: \\.id) { carPark in","                HStack {","                    VStack(alignment: .leading) {","                        Text(carPark.Development)","                            .font(.headline)","                        Text(carPark.Area)","                            .font(.subheadline)","                            .foregroundStyle(.secondary)","                    }","                    Spacer()","                    VStack(alignment: .trailing) {","                        Text(\"\\(carPark.AvailableLots)\")","                            .font(.title2.bold())","                            .foregroundStyle(availabilityColor(carPark.AvailableLots))","                        Text(\"lots\")","                            .font(.caption)","                            .foregroundStyle(.secondary)","                    }","                }","            }","            .searchable(text: $searchText, prompt: \"Search car parks\")","            .navigationTitle(\"Car Parks\")","            .refreshable {","                await loadCarParks()","            }","            .task {","                await loadCarParks()","            }","        }","    }","    ","    private func availabilityColor(_ lots: Int) -> Color {","        switch lots {","        case 0: return .red","        case 1...10: return .orange","        case 11...50: return .yellow","        default: return .green","        }","    }","    ","    private func loadCarParks() async {","        isLoading = true","        defer { isLoading = false }","        ","        do {","            carParks = try await LandTransportAPI.shared.downloadCarParkAvailability()","        } catch {","            print(\"Error: \\(error)\")","        }","    }","}"],"syntax":"swift"},{"anchor":"Bike-Parking","text":"Bike Parking","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"The bike parking API returns bicycle parking locations near a specified coordinate."}]},{"anchor":"Basic-Usage","text":"Basic Usage","type":"heading","level":3},{"type":"codeListing","code":["\/\/ Find bike parking within 500m of a location","let bikeParks = try await LandTransportAPI.shared.getBikeParks(","    near: 1.3521,      \/\/ latitude","    long: 103.8198,    \/\/ longitude","    radius: 0.5        \/\/ radius in km (default)",")"],"syntax":"swift"},{"anchor":"Bike-Park-Details","text":"Bike Park Details","type":"heading","level":3},{"code":["let bikePark = bikeParks.first!","","print(\"Description: \\(bikePark.Description)\")","print(\"Rack Type: \\(bikePark.RackType)\")","print(\"Rack Count: \\(bikePark.RackCount)\")","print(\"Shelter: \\(bikePark.ShelterIndicator)\")","","\/\/ Location","let location = bikePark.location","print(\"Location: \\(location.coordinate.latitude), \\(location.coordinate.longitude)\")"],"syntax":"swift","type":"codeListing"},{"anchor":"Finding-Bike-Parking-Near-MRT-Stations","text":"Finding Bike Parking Near MRT Stations","type":"heading","level":3},{"type":"codeListing","code":["import CoreLocation","","\/\/ Example: Find bike parking near Orchard MRT","let orchardMRT = CLLocationCoordinate2D(latitude: 1.3039, longitude: 103.8318)","","let nearbyBikeParks = try await LandTransportAPI.shared.getBikeParks(","    near: orchardMRT.latitude,","    long: orchardMRT.longitude,","    radius: 0.3  \/\/ 300 meters",")","","for park in nearbyBikeParks {","    print(\"\\(park.Description): \\(park.RackCount) racks\")","}"],"syntax":"swift"},{"anchor":"Estimated-Travel-Time","text":"Estimated Travel Time","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"text":"The estimated travel time API provides current travel times for major expressway routes.","type":"text"}]},{"anchor":"Basic-Usage","text":"Basic Usage","type":"heading","level":3},{"type":"codeListing","code":["let travelTimes = try await LandTransportAPI.shared.downloadEstimatedTravelTimes()","","for route in travelTimes {","    print(\"\\(route.Name): \\(route.EstTime) minutes\")","}"],"syntax":"swift"},{"anchor":"Travel-Time-Details","text":"Travel Time Details","type":"heading","level":3},{"type":"codeListing","code":["let route = travelTimes.first!","","print(\"Route: \\(route.Name)\")","print(\"Direction: \\(route.Direction)\")","print(\"Far End Point: \\(route.FarEndPoint)\")","print(\"Estimated Time: \\(route.EstTime) minutes\")"],"syntax":"swift"},{"anchor":"Travel-Time-Display","text":"Travel Time Display","type":"heading","level":3},{"type":"codeListing","code":["import SwiftUI","import LandTransportKit","","struct TravelTimesView: View {","    @State private var travelTimes: [EstimatedTravelTime] = []","    ","    var body: some View {","        List(travelTimes, id: \\.id) { route in","            HStack {","                VStack(alignment: .leading) {","                    Text(route.Name)","                        .font(.headline)","                    Text(\"To \\(route.FarEndPoint)\")","                        .font(.subheadline)","                        .foregroundStyle(.secondary)","                }","                Spacer()","                Text(\"\\(route.EstTime) min\")","                    .font(.title3.monospacedDigit())","                    .foregroundStyle(.blue)","            }","        }","        .navigationTitle(\"Travel Times\")","        .task {","            do {","                travelTimes = try await LandTransportAPI.shared.downloadEstimatedTravelTimes()","            } catch {","                print(\"Error: \\(error)\")","            }","        }","    }","}"],"syntax":"swift"},{"text":"Faulty Traffic Lights","anchor":"Faulty-Traffic-Lights","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"The faulty traffic lights API reports current traffic light malfunctions and scheduled maintenance."}],"type":"paragraph"},{"anchor":"Basic-Usage","type":"heading","level":3,"text":"Basic Usage"},{"syntax":"swift","type":"codeListing","code":["let faults = try await LandTransportAPI.shared.downloadFaultyTrafficLights()","","for fault in faults {","    print(\"\\(fault.AlarmID): \\(fault.TechnicalAlarmType) at \\(fault.NodeID)\")","}"]},{"anchor":"Fault-Details","type":"heading","level":3,"text":"Fault Details"},{"syntax":"swift","type":"codeListing","code":["let fault = faults.first!","","print(\"Alarm ID: \\(fault.AlarmID)\")","print(\"Node ID: \\(fault.NodeID)\")","print(\"Type: \\(fault.TechnicalAlarmType)\")","print(\"Start Date: \\(fault.StartDate)\")","print(\"End Date: \\(fault.EndDate)\")","print(\"Message: \\(fault.Message)\")","","\/\/ Check if this is scheduled maintenance","if fault.isScheduledMaintenance {","    print(\"This is scheduled maintenance\")","}","","\/\/ Get dates as Date objects","if let startDate = fault.iso8601StartDate {","    print(\"Started: \\(startDate)\")","}"]},{"text":"Road Events","anchor":"Road-Events","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"The road events APIs provide information about road openings and road works."}],"type":"paragraph"},{"text":"Road Openings","anchor":"Road-Openings","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["let openings = try await LandTransportAPI.shared.downloadRoadOpenings()","","for opening in openings {","    print(\"Event: \\(opening.EventID)\")","    print(\"Start: \\(opening.StartDate)\")","    print(\"End: \\(opening.EndDate)\")","}"]},{"text":"Road Works","anchor":"Road-Works","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["let works = try await LandTransportAPI.shared.downloadRoadWorks()","","for work in works {","    print(\"Event: \\(work.EventID)\")","    print(\"Road: \\(work.RoadName)\")","    print(\"Other: \\(work.Other)\")","}"]},{"text":"Facilities Maintenance","anchor":"Facilities-Maintenance","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"The facilities maintenance API provides information about lift maintenance at transport facilities."}],"type":"paragraph"},{"text":"Basic Usage","anchor":"Basic-Usage","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["let maintenance = try await LandTransportAPI.shared.downloadFacilitiesMaintenance()","","for item in maintenance {","    print(\"Station: \\(item.Station)\")","    print(\"Building: \\(item.Building)\")","}"]},{"text":"Passenger Volume Data","anchor":"Passenger-Volume-Data","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"The passenger volume APIs provide historical ridership data. These return ZIP files containing CSV data."}],"type":"paragraph"},{"name":"Warning","content":[{"inlineContent":[{"text":"These endpoints are rate-limited. Excessive requests will result in a ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPIError\/rateLimited"},{"text":" error.","type":"text"}],"type":"paragraph"}],"type":"aside","style":"warning"},{"text":"Available Endpoints","anchor":"Available-Endpoints","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["\/\/ Bus passenger volume by stop","let (busStopData, filename1) = try await LandTransportAPI.shared.downloadPassengerVolumeByBusStop()","","\/\/ Bus passenger volume by origin-destination","let (odBusData, filename2) = try await LandTransportAPI.shared.downloadPassengerVolumeByOriginDestinationBusStop()","","\/\/ Train passenger volume by station","let (trainStationData, filename3) = try await LandTransportAPI.shared.downloadPassengerVolumeByTrainStation()","","\/\/ Train passenger volume by origin-destination","let (odTrainData, filename4) = try await LandTransportAPI.shared.downloadPassengerVolumeByOriginDestinationTrainStation()"]},{"text":"Handling the ZIP Data","anchor":"Handling-the-ZIP-Data","type":"heading","level":3},{"inlineContent":[{"type":"text","text":"The returned data is in ZIP format containing a CSV file:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["import Foundation","","do {","    let (zipData, filename) = try await LandTransportAPI.shared.downloadPassengerVolumeByBusStop()","    ","    \/\/ Save to documents directory","    let documentsURL = FileManager.default.urls(for: .documentDirectory, in: .userDomainMask)[0]","    let fileURL = documentsURL.appendingPathComponent(filename)","    try zipData.write(to: fileURL)","    ","    print(\"Saved to: \\(fileURL.path)\")","    ","    \/\/ You can then unzip and parse the CSV as needed","    ","} catch LandTransportAPIError.rateLimited {","    print(\"Rate limited - please wait before trying again\")","} catch {","    print(\"Error: \\(error)\")","}"]},{"text":"Complete Example","anchor":"Complete-Example","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"Here’s a facilities dashboard combining multiple APIs:"}],"type":"paragraph"},{"syntax":"swift","type":"codeListing","code":["import SwiftUI","import LandTransportKit","","struct FacilitiesDashboardView: View {","    @State private var carParks: [CarPark] = []","    @State private var travelTimes: [EstimatedTravelTime] = []","    @State private var faultyLights: [FaultyTrafficLight] = []","    @State private var isLoading = false","    ","    var body: some View {","        NavigationStack {","            List {","                \/\/ Car Park Summary","                Section(\"Car Parks\") {","                    let available = carParks.filter { $0.AvailableLots > 0 }.count","                    let total = carParks.count","                    ","                    HStack {","                        Label(\"Available\", systemImage: \"car.fill\")","                        Spacer()","                        Text(\"\\(available) \/ \\(total)\")","                            .foregroundStyle(.secondary)","                    }","                }","                ","                \/\/ Travel Times","                Section(\"Expressway Travel Times\") {","                    ForEach(travelTimes.prefix(5), id: \\.id) { route in","                        HStack {","                            Text(route.Name)","                            Spacer()","                            Text(\"\\(route.EstTime) min\")","                                .foregroundStyle(.blue)","                        }","                    }","                }","                ","                \/\/ Faulty Traffic Lights","                Section(\"Traffic Light Issues (\\(faultyLights.count))\") {","                    if faultyLights.isEmpty {","                        Text(\"No reported issues\")","                            .foregroundStyle(.secondary)","                    } else {","                        ForEach(faultyLights.prefix(3), id: \\.AlarmID) { fault in","                            VStack(alignment: .leading) {","                                Text(fault.TechnicalAlarmType)","                                    .font(.headline)","                                Text(fault.Message)","                                    .font(.caption)","                                    .foregroundStyle(.secondary)","                            }","                        }","                    }","                }","            }","            .navigationTitle(\"Facilities\")","            .refreshable {","                await loadAllData()","            }","            .task {","                await loadAllData()","            }","        }","    }","    ","    private func loadAllData() async {","        isLoading = true","        defer { isLoading = false }","        ","        async let carParksTask = LandTransportAPI.shared.downloadCarParkAvailability()","        async let travelTimesTask = LandTransportAPI.shared.downloadEstimatedTravelTimes()","        async let faultyLightsTask = LandTransportAPI.shared.downloadFaultyTrafficLights()","        ","        do {","            let (parks, times, lights) = try await (carParksTask, travelTimesTask, faultyLightsTask)","            carParks = parks","            travelTimes = times","            faultyLights = lights","        } catch {","            print(\"Error: \\(error)\")","        }","    }","}"]}]}],"hierarchy":{"paths":[["doc:\/\/LandTransportKit\/documentation\/LandTransportKit"]]},"references":{"doc://LandTransportKit/documentation/LandTransportKit/BikePark":{"abstract":[{"type":"text","text":"A structure representing a bike parking location, including descriptive and geospatial information."}],"title":"BikePark","role":"symbol","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BikePark","kind":"symbol","type":"topic","navigatorTitle":[{"text":"BikePark","kind":"identifier"}],"url":"\/documentation\/landtransportkit\/bikepark","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"BikePark"}]},"doc://LandTransportKit/documentation/LandTransportKit/LandTransportAPI/downloadRoadOpenings()":{"role":"symbol","kind":"symbol","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"downloadRoadOpenings","kind":"identifier"},{"text":"() ","kind":"text"},{"text":"async","kind":"keyword"},{"text":" ","kind":"text"},{"text":"throws","kind":"keyword"},{"text":" -> [","kind":"text"},{"preci