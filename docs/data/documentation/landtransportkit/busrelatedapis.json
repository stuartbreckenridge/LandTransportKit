{"abstract":[{"text":"Access bus arrival times, routes, services, and stop information.","type":"text"}],"identifier":{"url":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusRelatedAPIs","interfaceLanguage":"swift"},"metadata":{"title":"Bus Related APIs","images":[{"type":"icon","identifier":"bus.png"}],"color":{"standardColorIdentifier":"green"},"roleHeading":"API Collection","modules":[{"name":"LandTransportKit"}],"role":"collectionGroup"},"schemaVersion":{"minor":3,"major":0,"patch":0},"topicSections":[{"identifiers":["doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusArrivals","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/Services","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/NextBus","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusService","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusRoute","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/PlannedBusRoute","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusStop"],"anchor":"Models","title":"Models"}],"primaryContentSections":[{"content":[{"text":"Overview","anchor":"Overview","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"LandTransportKit provides comprehensive access to Singapore’s bus network data through four main APIs:"}]},{"rows":[[[{"type":"paragraph","inlineContent":[{"type":"text","text":"API"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Method"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Description"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"Bus Arrivals"}]}],[{"type":"paragraph","inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/getBusArrivals(at:serviceNo:)"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Real-time arrival times"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"Bus Routes"}]}],[{"type":"paragraph","inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadBusRoutes()"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Complete route information"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"Bus Services"}]}],[{"type":"paragraph","inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadBusServices()"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"Service details and frequency"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"Bus Stops"}]}],[{"type":"paragraph","inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadBusStops()"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"All bus stop locations"}]}]]],"header":"row","type":"table"},{"text":"Bus Arrivals","anchor":"Bus-Arrivals","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"The bus arrivals API provides real-time estimated arrival times for buses at a specific stop."}]},{"text":"Basic Usage","anchor":"Basic-Usage","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["import LandTransportKit","","\/\/ Get arrivals for all services at a stop","let arrivals = try await LandTransportAPI.shared.getBusArrivals(at: \"08057\")","","\/\/ Get arrivals for a specific service only","let service106 = try await LandTransportAPI.shared.getBusArrivals(at: \"08057\", serviceNo: \"106\")"]},{"text":"Working with Arrival Data","anchor":"Working-with-Arrival-Data","type":"heading","level":3},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusArrivals"},{"type":"text","text":" response contains an array of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/Services"},{"type":"text","text":" objects, each representing a bus service at the stop:"}]},{"syntax":"swift","type":"codeListing","code":["@Observable","final class BusArrivalsViewModel {","    var arrivals: [Services] = []","    var isLoading = false","    var error: String?","    ","    func loadArrivals(for stopCode: String) async {","        isLoading = true","        defer { isLoading = false }","        ","        do {","            let response = try await LandTransportAPI.shared.getBusArrivals(at: stopCode)","            arrivals = response.Services","        } catch let apiError as LandTransportAPIError {","            error = apiError.localizedDescription","        } catch {","            error = \"Failed to load arrivals\"","        }","    }","}"]},{"text":"Arrival Time Information","anchor":"Arrival-Time-Information","type":"heading","level":3},{"type":"paragraph","inlineContent":[{"type":"text","text":"Each service provides up to three "},{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/NextBus"},{"type":"text","text":" objects representing the next arriving buses:"}]},{"syntax":"swift","type":"codeListing","code":["let service = arrivals.Services.first!","","\/\/ Next three buses","let nextBus = service.NextBus      \/\/ First arriving bus","let nextBus2 = service.NextBus2    \/\/ Second arriving bus","let nextBus3 = service.NextBus3    \/\/ Third arriving bus","","\/\/ Get arrival time as Date","if let arrivalDate = nextBus.estimatedArrivalDate {","    let formatter = RelativeDateTimeFormatter()","    print(\"Arriving \\(formatter.localizedString(for: arrivalDate, relativeTo: Date()))\")","}","","\/\/ Get bus location","if let location = nextBus.location {","    print(\"Bus is at: \\(location.coordinate.latitude), \\(location.coordinate.longitude)\")","}","","\/\/ Check bus load","print(\"Load: \\(nextBus.Load)\")  \/\/ \"SEA\" (Seats Available), \"SDA\" (Standing Available), \"LSD\" (Limited Standing)"]},{"text":"Bus Routes","anchor":"Bus-Routes","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"The bus routes API returns the complete sequence of stops for every bus service."}]},{"text":"Downloading All Routes","anchor":"Downloading-All-Routes","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["let routes = try await LandTransportAPI.shared.downloadBusRoutes()","","\/\/ Filter routes for a specific service","let service106Routes = routes.filter { $0.ServiceNo == \"106\" }","","\/\/ Get stops in order for direction 1","let direction1Stops = service106Routes","    .filter { $0.Direction == 1 }","    .sorted { $0.StopSequence < $1.StopSequence }","","for stop in direction1Stops {","    print(\"\\(stop.StopSequence). \\(stop.BusStopCode) - Distance: \\(stop.Distance)km\")","}"]},{"text":"Working with Timing Information","anchor":"Working-with-Timing-Information","type":"heading","level":3},{"type":"paragraph","inlineContent":[{"type":"text","text":"Each "},{"type":"reference","isActive":true,"identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusRoute"},{"type":"text","text":" includes first and last bus times:"}]},{"syntax":"swift","type":"codeListing","code":["let route = routes.first!","","print(\"Weekday: \\(route.WD_FirstBus) - \\(route.WD_LastBus)\")","print(\"Saturday: \\(route.SAT_FirstBus) - \\(route.SAT_LastBus)\")","print(\"Sunday: \\(route.SUN_FirstBus) - \\(route.SUN_LastBus)\")"]},{"text":"Planned Bus Routes","anchor":"Planned-Bus-Routes","type":"heading","level":3},{"type":"paragraph","inlineContent":[{"type":"text","text":"For upcoming route changes, use the planned routes API:"}]},{"syntax":"swift","type":"codeListing","code":["let plannedRoutes = try await LandTransportAPI.shared.downloadPlannedBusRoutes()","","for route in plannedRoutes {","    print(\"Service \\(route.ServiceNo) changes effective: \\(route.EffectiveDate)\")","}"]},{"text":"Bus Services","anchor":"Bus-Services","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"The bus services API provides details about each bus service including operator, category, and frequency."}]},{"text":"Downloading All Services","anchor":"Downloading-All-Services","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["let services = try await LandTransportAPI.shared.downloadBusServices()","","\/\/ Group by operator","let byOperator = Dictionary(grouping: services) { $0.Operator }","for (operator, operatorServices) in byOperator {","    print(\"\\(operator): \\(operatorServices.count) services\")","}"]},{"text":"Service Details","anchor":"Service-Details","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["let service = services.first { $0.ServiceNo == \"106\" && $0.Direction == 1 }!","","print(\"Service: \\(service.ServiceNo)\")","print(\"Operator: \\(service.Operator)\")           \/\/ \"SBST\", \"SMRT\", etc.","print(\"Category: \\(service.Category)\")           \/\/ \"TRUNK\", \"FEEDER\", etc.","print(\"Origin: \\(service.OriginCode)\")","print(\"Destination: \\(service.DestinationCode)\")","","\/\/ Frequency information","print(\"AM Peak: \\(service.AM_Peak_Freq)\")        \/\/ e.g., \"08-12\" (minutes)","print(\"AM Off-Peak: \\(service.AM_Offpeak_Freq)\")","print(\"PM Peak: \\(service.PM_Peak_Freq)\")","print(\"PM Off-Peak: \\(service.PM_Offpeak_Freq)\")"]},{"text":"Bus Stops","anchor":"Bus-Stops","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"The bus stops API returns all bus stop locations in Singapore."}]},{"text":"Downloading All Stops","anchor":"Downloading-All-Stops","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["let stops = try await LandTransportAPI.shared.downloadBusStops()","print(\"Total stops: \\(stops.count)\")  \/\/ ~5000 stops"]},{"text":"Finding Nearby Stops","anchor":"Finding-Nearby-Stops","type":"heading","level":3},{"type":"paragraph","inlineContent":[{"type":"text","text":"Use the built-in location properties to find stops near a location:"}]},{"syntax":"swift","type":"codeListing","code":["import CoreLocation","","let userLocation = CLLocation(latitude: 1.3521, longitude: 103.8198)  \/\/ Singapore","","let nearbyStops = stops.filter { stop in","    stop.location.distance(from: userLocation) <= 500  \/\/ Within 500 meters","}","","for stop in nearbyStops.sorted(by: { $0.location.distance(from: userLocation) < $1.location.distance(from: userLocation) }) {","    let distance = stop.location.distance(from: userLocation)","    print(\"\\(stop.Description) (\\(stop.BusStopCode)) - \\(Int(distance))m\")","}"]},{"text":"Stop Details","anchor":"Stop-Details","type":"heading","level":3},{"syntax":"swift","type":"codeListing","code":["let stop = stops.first { $0.BusStopCode == \"08057\" }!","","print(\"Code: \\(stop.BusStopCode)\")","print(\"Name: \\(stop.Description)\")","print(\"Road: \\(stop.RoadName)\")","","\/\/ Get coordinates","if let coordinate = stop.coordinate2D {","    print(\"Location: \\(coordinate.latitude), \\(coordinate.longitude)\")","}"]},{"text":"Complete Example","anchor":"Complete-Example","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"Here’s a complete SwiftUI view that displays bus arrivals with auto-refresh:"}]},{"syntax":"swift","type":"codeListing","code":["import SwiftUI","import LandTransportKit","","struct BusStopView: View {","    let stopCode: String","    ","    @State private var arrivals: BusArrivals?","    @State private var isLoading = false","    @State private var error: LandTransportAPIError?","    ","    var body: some View {","        List {","            if let arrivals = arrivals {","                Section {","                    Text(arrivals.BusStopCode)","                        .font(.caption)","                        .foregroundStyle(.secondary)","                }","                ","                ForEach(arrivals.Services) { service in","                    BusServiceRow(service: service)","                }","            }","        }","        .overlay {","            if isLoading && arrivals == nil {","                ProgressView()","            }","            ","            if let error = error {","                ContentUnavailableView {","                    Label(\"Error\", systemImage: \"exclamationmark.triangle\")","                } description: {","                    Text(error.localizedDescription)","                } actions: {","                    Button(\"Retry\") {","                        Task { await loadArrivals() }","                    }","                }","            }","        }","        .refreshable {","            await loadArrivals()","        }","        .task {","            await loadArrivals()","        }","        .task(id: \"refresh\") {","            \/\/ Auto-refresh every 30 seconds","            while !Task.isCancelled {","                try? await Task.sleep(for: .seconds(30))","                await loadArrivals()","            }","        }","    }","    ","    private func loadArrivals() async {","        isLoading = true","        defer { isLoading = false }","        ","        do {","            arrivals = try await LandTransportAPI.shared.getBusArrivals(at: stopCode)","            error = nil","        } catch let apiError as LandTransportAPIError {","            error = apiError","        } catch {","            \/\/ Handle unexpected errors","        }","    }","}","","struct BusServiceRow: View {","    let service: Services","    ","    var body: some View {","        HStack {","            Text(service.ServiceNo)","                .font(.title2.bold())","                .frame(width: 60, alignment: .leading)","            ","            Spacer()","            ","            HStack(spacing: 16) {","                ArrivalBadge(nextBus: service.NextBus)","                ArrivalBadge(nextBus: service.NextBus2)","                ArrivalBadge(nextBus: service.NextBus3)","            }","        }","        .padding(.vertical, 4)","    }","}","","struct ArrivalBadge: View {","    let nextBus: NextBus","    ","    var body: some View {","        if nextBus.EstimatedArrival.isEmpty {","            Text(\"--\")","                .foregroundStyle(.secondary)","        } else if let minutes = nextBus.minutesToArrival {","            Text(minutes <= 0 ? \"Arr\" : \"\\(minutes)\")","                .font(.callout.monospacedDigit())","                .padding(.horizontal, 8)","                .padding(.vertical, 4)","                .background(loadColor)","                .foregroundStyle(.white)","                .clipShape(RoundedRectangle(cornerRadius: 6))","        }","    }","    ","    private var loadColor: Color {","        switch nextBus.Load {","        case \"SEA\": return .green","        case \"SDA\": return .orange","        case \"LSD\": return .red","        default: return .gray","        }","    }","}"]}],"kind":"content"}],"kind":"article","hierarchy":{"paths":[["doc:\/\/LandTransportKit\/documentation\/LandTransportKit"]]},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/landtransportkit\/busrelatedapis"]}],"sections":[],"seeAlsoSections":[{"generated":true,"identifiers":["doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusArrivals","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusService","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusRoute","doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/BusStop"],"anchor":"Bus","title":"Bus"}],"references":{"doc://LandTransportKit/documentation/LandTransportKit/LandTransportAPI/downloadBusServices()":{"abstract":[{"tex