"role":"symbol","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"downloadTaxiStands"},{"kind":"text","text":"() "},{"kind":"keyword","text":"async"},{"kind":"text","text":" "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> ["},{"preciseIdentifier":"s:16LandTransportKit9TaxiStandV","kind":"typeIdentifier","text":"TaxiStand"},{"kind":"text","text":"]"}],"abstract":[{"type":"text","text":"Downloads the list of taxi stands from the Land Transport API."}],"type":"topic"},"doc://LandTransportKit/documentation/LandTransportKit/TaxiStand":{"title":"TaxiStand","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/TaxiStand","kind":"symbol","url":"\/documentation\/landtransportkit\/taxistand","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"TaxiStand"}],"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"TaxiStand"}],"abstract":[{"type":"text","text":"Represents a taxi stand location with associated details."}],"type":"topic"},"doc://LandTransportKit/documentation/LandTransportKit":{"images":[{"identifier":"LTK-Red.png","type":"icon"}],"title":"LandTransportKit","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit","abstract":[{"type":"text","text":"A Swift package for accessing real-time and static data from Singapore’s Land Transport Authority (LTA) DataMall API."}],"type":"topic","url":"\/documentation\/landtransportkit","kind":"symbol","role":"collection"},"doc://LandTransportKit/documentation/LandTransportKit/LandTransportAPI/downloadTaxiAvailability()":{"url":"\/documentation\/landtransportkit\/landtransportapi\/downloadtaxiavailability()","role":"symbol","type":"topic","kind":"symbol","identifier":"doc:\/\/LandTransportKit\/documentation\/LandTransportKit\/LandTransportAPI\/downloadTaxiAvailability()","title":"downloadTaxiAvailability()","abstract":[{"type":"text","text":"Retrieves the current locations of available taxis."}],"fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"downloadTaxiAvailability"},{"kind":"text","text":"() "},{"kind":"keyword","text":"async"},{"kind":"text","text":" "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> ["},{"preciseIdentifier":"s:16LandTransportKit16TaxiAvailabilityV","kind":"typeIdentifier","text":"TaxiAvailability"},{"kind":"text","text":"]"}]}}}